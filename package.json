{
  "name": "RNFirebaseTest",
  "version": "5.1.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "fs-extra": "^6.0.1",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "react-test-renderer": "16.6.3",
    "replace-in-file": "^3.4.2"
  },
  "scripts": {
    "android-bundle": "mkdir android/app/src/main/assets && npx react-native bundle --platform android --dev true --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android": "react-native run-android",
    "old-android": "npm run android-bundle && npm run android",
    "ios": "react-native run-ios --simulator=\"iPhone X\"",
    "apk": "cd android && ./gradlew assembleRelease",
    "rename": "node ./bin/rename.js",
    "start": "react-native start",
    "test": "jest"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "formik": "^1.5.1",
    "react": "16.6.3",
    "react-native": "0.58.6",
    "react-native-firebase": "^5.2.0"
  }
}
